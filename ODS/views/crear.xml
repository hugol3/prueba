<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record model="ir.ui.view" id="usuario_form">
        <field name="name">Usuario</field>
        <field name="model">usuario.model</field>
        <field name="priority" eval="16"/>
        <field name="arch" type="xml">
            <form>
                <sheets>
                    <group>
                        <group>
                            <field name="name"/>
                        </group>
                        <group>
                            <field name="correo"/>
                        </group>
                    </group>
                </sheets>
            </form>
        </field>
    </record>    




</odoo>



